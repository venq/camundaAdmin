{
  "type": "guaranteeInterestRate",
  "uiType": "uniform",
  "visible": true,
  "readonly": true,
  "cacheModel": false,
  "collapse": false,
  "gutter": [ 16, 16 ],
  "blocks": [
    {
      "pos": 1,
      "id": "interestRateParams",
      "uiType": "wrapper",
      "visible": true,
      "readonly": false,
      "collapse": false,
      "fields": [
        {
          "col": 24,
          "pos": 1,
          "name": "rateType",
          "type": "select",
          "label": "Вид процентной ставки",
          "visible": true,
          "readonly": false,
          "required": true,
          "properties": {
            "options": [
              { "key": "fixed", "value": "Фиксированное значение" },
              { "key": "doubled", "value": "Удвоенная ставка" },
              { "key": "rateWithValue", "value": "Ставка + значение" }
            ]
          },
          "responsive": { "lg": 24, "md": 24, "sm": 24, "xl": 24, "xs": 24, "xxl": 24 }
        },
        {
          "col": 24,
          "pos": 2,
          "name": "rateIndicator",
          "type": "select",
          "label": "Индикатор",
          "visibleExp": "component?.rateType === 'doubled' || component?.rateType === 'rateWithValue'",
          "readonly": false,
          "required": true,
          "properties": {
            "defaultValue": "RUSSIAN_KEY",
            "options": [
              { "key": "RUSSIAN_KEY", "value": "Ключевая ставка Банка России" }
            ]
          },
          "responsive": { "lg": 24, "md": 24, "sm": 24, "xl": 24, "xs": 24, "xxl": 24 }
        },
        {
          "col": 8,
          "pos": 3,
          "name": "rateInterest",
          "type": "inputNumber",
          "label": "Значение индикатора",
          "visibleExp": "component?.rateType === 'doubled' || component?.rateType === 'rateWithValue'",
          "readonly": false,
          "required": true,
          "properties": {
            "min": 0,
            "max": 100
          },
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 8,
          "pos": 4,
          "name": "rateCoefficient",
          "type": "inputNumber",
          "label": "Коэффициент",
          "visibleExp": "component?.rateType === 'doubled'",
          "readonly": false,
          "required": true,
          "properties": {
            "min": 0,
            "max": 100
          },
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 6,
          "pos": 4,
          "name": "rateAllowance",
          "type": "inputNumber",
          "label": "Надбавка",
          "visibleExp": "component?.rateType === 'rateWithValue'",
          "readonly": false,
          "required": true,
          "properties": {
            "min": 0,
            "max": 100
          },
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 8,
          "pos": 5,
          "name": "rateValue",
          "type": "inputNumber",
          "label": "Процентная ставка по БГ",
          "visibleExp": "component?.rateType === 'fixed' || component?.rateType === 'doubled' || component?.rateType === 'rateWithValue'",
          "disabledExp": "component?.rateType === 'doubled' || component?.rateType === 'rateWithValue'",
          "requireExp": "component?.rateType === 'fixed'",
          "readonly": false,
          "valueExp": "component?.rateType === 'doubled' ? (component?.rateInterest * component?.rateCoefficient) : (component?.rateType === 'rateWithValue' ? (component?.rateInterest + component?.rateAllowance) : component?.rateValue)",
          "properties": {
            "min": 0,
            "max": 100
          },
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 12,
          "pos": 6,
          "name": "rateRecalcInterest",
          "type": "select",
          "label": "Пересчет % при изменении индикатора",
          "visibleExp": "component?.rateType === 'doubled' || component?.rateType === 'rateWithValue'",
          "readonly": false,
          "required": false,
          "properties": {
            "options": [
              { "key": "1", "value": "С даты изменения" },
              { "key": "3", "value": "Со следующего дня после даты изменений" }
            ]
          },
          "responsive": { "lg": 12, "md": 24, "sm": 24, "xl": 12, "xs": 24, "xxl": 12 }
        }
      ]
    }
  ]
}
