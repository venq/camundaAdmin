{
  "type": "licenses",
  "title": "Лицензии поставщика",
  "uiType": "unitable",
  "visible": true,
  "readonly": true,
  "collapse": true,
  "uiConfig": {
    "actions": [
      {
        "action": "add",
        "type": "top-action",
        "title": "Добавить лицензию"
      },
      {
        "action": "edit",
        "type": "row-action",
        "visibleExp": "component?.source !== 'enrich'"
      },
      {
        "action": "remove",
        "type": "row-action",
        "visibleExp": "component?.source !== 'enrich'"
      }
    ],
    "datasource": {
      "type": "model",
      "property": "items"
    },
    "indexColumn": {
      "enabled": false
    },
    "columns": [
      {
        "header": "Номер лицензии",
        "field": "number",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "type": "text",
        "required": true,
        "readonlyExp": "component?.source == 'enrich'"
      },
      {
        "header": "Перечень видов лицензируемой деятельности",
        "field": "services",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "type": "text",
        "required": true,
        "readonlyExp": "component?.source == 'enrich'"
      },
      {
        "header": "Дата выдачи",
        "field": "issuedDate",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "type": "date",
        "format": "DD.MM.YYYY",
        "required": true,
        "readonlyExp": "component?.source == 'enrich'"
      },
      {
        "header": "Дата окончания",
        "field": "expiredDate",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "type": "date",
        "format": "DD.MM.YYYY",
        "required": false,
        "readonlyExp": "component?.source == 'enrich'"
      },
      {
        "header": "Статус",
        "field": "status",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "type": "select",
        "required": true,
        "options": [
          {
            "value": "Действующая",
            "key": "Действующая"
          },
          {
            "value": "Ограничена",
            "key": "Ограничена"
          },
          {
            "value": "Приостановлена",
            "key": "Приостановлена"
          },
          {
            "value": "Возобновлена",
            "key": "Возобновлена"
          },
          {
            "value": "Отозвана",
            "key": "Отозвана"
          },
          {
            "value": "Недействующая",
            "key": "Недействующая"
          }
        ],
        "readonlyExp": "component?.source == 'enrich'"
      },
      {
        "header": "Источник",
        "field": "source",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "readonly": true,
        "type": "text",
        "required": true,
        "visible": false,
        "defaultValue": "manual"
      },
      {
        "header": "Относится к основной деятельности",
        "field": "activityMain",
        "sorting": {
          "enabled": true,
          "default": "ASC"
        },
        "readonly": false,
        "type": "switch",
        "required": true
      }
    ]
  }
}