{
  "type": "uni",
  "uiType": "uniform",
  "keyFormat": "publicationParams~${orderId}",
  "visible": true,
  "readonly": true,
  "collapse": false,
  "blocks": [
    {
      "id": "publicationParamsBlock1",
      "uiType": "wrapper",
      "visible": true,
      "readonly": false,
      "collapse": false,
      "fields": [
        {
          "col": 24,
          "pos": 1,
          "name": "principalInn",
          "type": "input",
          "label": "ИНН принципала",
          "visible": true,
          "requireExp": "!!!model?.bgOrder?.clientInfo?.info?.inn",
          "disabledExp": "model?.bgOrder?.clientInfo?.info?.inn?.length > 0",
          "defaultValueExp": "model?.bgOrder?.clientInfo?.info?.inn",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 24,
          "pos": 2,
          "name": "principalKpp",
          "type": "input",
          "label": "КПП принципала",
          "visible": true,
          "requireExp": "!!!model?.bgOrder?.clientInfo?.info?.kpp",
          "disabledExp": "model?.bgOrder?.clientInfo?.info?.kpp?.length > 0",
          "defaultValueExp": "model?.bgOrder?.clientInfo?.info?.kpp",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 24,
          "pos": 3,
          "name": "principalOktmo",
          "type": "input",
          "label": "ОКТМО принципала",
          "visible": true,
          "readonly": false,
          "required": true,
          "defaultValueExp": "model?.bgOrder?.clientInfo?.info?.oktmo",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        }
      ]
    },
    {
      "id": "publicationParamsBlock2",
      "uiType": "wrapper",
      "visibleExp": "model?.bgOrder?.productInfo?.law === '44' || model?.bgOrder?.productInfo?.law === '223'",
      "readonly": false,
      "collapse": false,
      "fields": [
        {
          "col": 24,
          "pos": 1,
          "name": "beneficiaryInn",
          "type": "input",
          "label": "ИНН бенефициара",
          "visible": true,
          "requireExp": "!!!model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.inn",
          "disabledExp": "model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.inn?.length > 0",
          "defaultValueExp": "model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.inn",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 24,
          "pos": 2,
          "name": "beneficiaryKpp",
          "type": "input",
          "label": "КПП бенефициара",
          "visible": true,
          "requireExp": "!!!model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.kpp",
          "disabledExp": "model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.kpp?.length > 0",
          "defaultValueExp": "model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.kpp",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        },
        {
          "col": 24,
          "pos": 3,
          "name": "beneficiaryOktmo",
          "type": "input",
          "label": "ОКТМО бенефициара",
          "visible": true,
          "readonly": false,
          "required": true,
          "defaultValueExp": "model?.bgOrder?.productInfo?.lots?.[0]?.beneficiaries?.[0]?.oktmo",
          "responsive": { "lg": 8, "md": 24, "sm": 24, "xl": 8, "xs": 24, "xxl": 8 }
        }
      ]
    }
  ]
}
